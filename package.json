{
  "name": "meetomeal",
  "private": true,
  "dependencies": {
    "@rails/actioncable": "^6.0.0",
    "@rails/activestorage": "^6.0.0",
    "@rails/ujs": "^6.0.0",
    "@rails/webpacker": "4.2.2",
    "turbolinks": "^5.2.0",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "version": "0.1.0",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "jest": "^26.1.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "automock": false,
    "clearMocks": true,
    "moduleDirectories": [
      "app/javascript",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "tests/__mocks__/styleMock.js"
    },
    "roots": [
      "app/javascript"
    ],
    "setupFiles": [
      "./app/javascript/tests/setupJest.js"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }

}
